Punto1

#include <iostream>

int main()
{
	int num1, num2;
	std::cout << "ingresar dos numeros: " << std::endl;
	std::cin >> num1 >> num2;
	
	if(num1<num2)
	{ 
		std::cout << "el numero mayor es " << num2 << std::endl;
	}
	
	if(num1>num2)
	{ 
		std::cout << "el numero mayor es " << num1 << std::endl;
	}
	
	return 0;
}
